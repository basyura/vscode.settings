{
  "vim.leader": "m",
  "vim.timeout": 500,
  "vim.useSystemClipboard": true,
  "vim.scroll": 20,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "<C-j>"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "<C-e>"
      ],
      "after": [
        "<Esc>",
        "$",
        "a"
      ]
    },
    {
      "before": [
        "<C-a>"
      ],
      "after": [
        "<Esc>",
        "0",
        "i"
      ]
    },
    {
      "before": [
        "<C-d>"
      ],
      "after": [
        "<Del>"
      ]
    },
    {
      "before": [
        "<C-o>"
      ],
      "after": [
        "<Esc>",
        "o"
      ]
    },
    {
      "before": [
        "<C-k>"
      ],
      "after": [],
      "commands": [
        {
          "command": "deleteAllRight",
          "args": []
        }
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<C-j>"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "<C-e>"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "0"
      ],
      "after": [
        "^"
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [],
      "commands": [
        {
          "command": "editor.action.showDefinitionPreviewHover",
          "args": []
        }
      ]
    },
    {
      "before": [
        "<C-n>"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.quickOpen",
          "args": []
        }
      ]
    },
    {
      "before": [
        "I"
      ],
      "after": [
        "i",
        " ",
        "<Left>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<Leader>",
        "c"
      ],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    },
    {
      "before": [
        "<Leader>",
        "r"
      ],
      "commands": [
        {
          "command": "editor.action.rename"
        }
      ]
    },
    {
      "before": [
        "<C-k>"
      ],
      "commands": [
        {
          "command": "extension.vim_ctrl+]"
        }
      ]
    },
    {
      "before": [
        "u"
      ],
      "commands": [
        {
          "command": "undo"
        }
      ]
    },
    {
      "before": [
        "U"
      ],
      "commands": [
        {
          "command": "redo"
        }
      ]
    },
    {
      "before":["w","l"],
      "after": ["<C-w>","l"]
    },
    {
      "before":["w","h"],
      "after": ["<C-w>","h"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "<C-j>"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "v"
      ],
      "after": [
        "$",
        "h"
      ]
    },
    {
      "before": [
        "<C-e>"
      ],
      "after": [
        "$",
        "h"
      ]
    },
    {
      "before": [
        "<Leader>",
        "c"
      ],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    }
  ],
  "editor.fontSize": 14,
  "editor.lineHeight": 20,
  "terminal.integrated.fontSize": 13,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "workbench.colorCustomizations": {
    "editorSuggestWidget.selectedBackground": "#5e81ac",
    "editorSuggestWidget.highlightForeground": "#ff9200",
    "list.focusBackground": "#ff000040"
  },
  "outline.showVariables": false,
  "go.useLanguageServer": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "search.quickOpen.includeHistory": false,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.showIcons": false,
  "workbench.iconTheme": null,
  "prettier.disableLanguages": [
    "markdown"
  ],
  "workbench.statusBar.visible": true,
  "editor.cursorSurroundingLines": 5,
  "editor.parameterHints.enabled": false,
  "python": {
    "editor.formatOnSave": true,
    "jediEnabled": false,
    "languageServer": "Microsoft",
    "showStartPage": false,
    "pythonPath": "~/.pyenv/shims/python"
  },
  "editor.fontFamily": "HackGen, Consolas, 'Courier New', monospace",
  "workbench.sideBar.location": "right",
  "workbench.activityBar.location": "top"
}
